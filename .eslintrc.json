{
  "extends": ["airbnb", "prettier"],
  "env": {
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "./"
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/extensions": [0, "never"]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
